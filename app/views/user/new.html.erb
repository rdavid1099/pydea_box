<%= form_for @user do |f| %>
  <%= f.label :email, 'Email Address' %><br>
  <%= f.text_field :email, placeholder: 'Email Address' %><br>
  <%= f.label :username %><br>
  <%= f.text_field :username, placeholder: 'Username' %><br>
  <%= f.label :password %><br>
  <%= f.password_field :password, placeholder: 'Password' %><br>
  <%= f.label :password_confirmation %><br>
  <%= f.password_field :password_confirmation, placeholder: 'Password Confirmation' %><br>
  <%= f.submit 'Create Account' %>
<% end %>
